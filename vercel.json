{
    "version": 2,
    "builds": [
        { "src": "*.html", "use": "@vercel/static" },
        { "src": "*.py", "use": "@vercel/python" },
        { "src": "*.js", "use": "@vercel/node" }
      ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/",
            "dest": "/index.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/cotacao",
            "dest": "/index.js",
            "methods": [
                "GET"
            ]
        }
    ]
}